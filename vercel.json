{
    "version": 2,
    "installCommand": "yarn install",
    "buildCommand": "turbo run build --filter=auth...",
    "outputDirectory": "apps/microservices/auth/dist",
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        }
    ]
}